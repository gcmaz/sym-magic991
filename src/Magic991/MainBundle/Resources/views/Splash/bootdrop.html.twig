{% extends 'Magic991MainBundle::layout.html.twig' %}

{% block title %}
    New Years Eve Boot Drop 2014 with Magic 99.1 Radio - Prescott, AZ
{% endblock %}

{% block description %}
    <meta name="description" content="New Years Eve Boot Drop 2014 with Magic 99.1 Radio - Prescott, AZ"/>
    <meta property="og:site_name" content="http://magic991.gcmaz.com" />
    <meta property="fb:admins" content="100003228578475" />
    <meta property="fb:app_id" content="388413197938552" />
    <meta property="og:type" content="music.radio_station" />
    <meta property="og:image" content="http://magic991.gcmaz.com{{ asset('images/660x250/boot-drop-2014.jpg') }}"/>
